<%if @daily_news %>
<ul class="collapsible ">
  <li>
    <div class="collapsible-header"><i class="material-icons">local_library</i>Jornal Mural</div>
    <div class="collapsible-body colapsible-remove-margin">
			<div class='carousel'>
          <% @daily_news.img_news.each_with_index do |img, i| %>
              <a class="carousel-item" href="#<%= i.humanize %>!"><%= image_tag  img %></a>		
          <%end%>
      </div>
    </div>
  </li>
</ul>
<%end%>
