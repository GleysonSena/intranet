<%  title = params[:filter]  %>

<div class="row">
<center><h3>Resumo Relat√≥rios <%= title %></h3></center>
  <% @reports.each do |report|%>
    <% report.report_image_data.each do |image|%>
      <% params[:filter] == 'consolidado' ? filter = @lastlocation : filter = current_user.location_id %>
      <% if  image.location_id == filter %>
        <p> <%= image_tag image.image, class:'responsive-img col s12' %> </p>
      <% end %>
    <% end %>
  <% end %>
</div>


