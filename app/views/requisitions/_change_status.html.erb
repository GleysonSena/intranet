<div id="edit_status" class="modal">
  <div class="modal-content">
    <h4>Atender Requisição</h4>
    <%= @requisition.requisition_status_id > 1 ?  (render 'requisitions/shared/end_status_form', requisition: @requisition) :  (render 'requisitions/shared/start_status_form', requisition: @requisition) %>
    <p><%= %></p>
  </div>
  <div class="modal-footer">
    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Cancelar</a>
  </div>
</div>